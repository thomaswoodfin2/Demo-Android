<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
<RelativeLayout

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
       >
    <include
        android:id="@+id/layout_Header"
        layout="@layout/item_header"
        ></include>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/layout_Header"
        android:layout_marginTop="20dp"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rl_container"
        android:orientation="vertical"
        >
    <ImageView
        android:id="@+id/iv_logo"
        android:layout_width="@dimen/dim_50"
        android:layout_height="100dp"
        android:background="@mipmap/ic_login"
        android:layout_gravity="center_horizontal"
        />



<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/ll_dateContainer"
    android:layout_marginTop="@dimen/dim_15"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/ll_planData"
        android:layout_marginLeft="@dimen/dim_20"
        android:layout_marginRight="@dimen/dim_20"
        >
        <TextView
            android:id="@+id/tv_planAmountLabel"
            android:layout_width="0dp"
            android:layout_weight="50"
            android:layout_height="wrap_content"
            style="@style/textview_14_grey"
            android:textColor="@color/grey_color"
            android:text="Plan Amount"
            android:singleLine="false"
            android:maxLines="2"
            />
        <TextView
            android:id="@+id/tv_planAmountValue"
            android:layout_width="0dp"
            android:layout_weight="50"
            android:singleLine="false"
            android:maxLines="2"
            android:layout_height="wrap_content"
            style="@style/textview_14_grey"
            android:textColor="@color/grey_color"
            android:text="Start Date"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_1"
        android:background="@color/notificationborder"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="@dimen/dim_20"
        android:layout_marginRight="@dimen/dim_20"
        android:id="@+id/view4"
        ></View>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/ll_startDate"
            android:layout_below="@+id/tv_Plan"
            android:layout_marginLeft="@dimen/dim_20"
            android:layout_marginRight="@dimen/dim_20"
            android:layout_marginTop="@dimen/dim_10"
            >
            <TextView
            android:id="@+id/tv_startDateLabel"
                android:layout_width="0dp"
                android:layout_weight="50"
            android:layout_height="wrap_content"
            style="@style/textview_14_grey"
            android:textColor="@color/grey_color"
            android:text="Plan Start Date"
                android:singleLine="false"
                android:maxLines="2"
            />
        <TextView
            android:id="@+id/tv_startDateValue"
            android:layout_width="0dp"
            android:layout_weight="50"
            android:singleLine="false"
            android:maxLines="2"
            android:layout_height="wrap_content"
            style="@style/textview_14_grey"
            android:textColor="@color/grey_color"
            android:text="Start Date"
            />
        </LinearLayout>
<View
    android:layout_width="match_parent"
    android:layout_height="@dimen/dim_1"
    android:background="@color/notificationborder"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="@dimen/dim_20"
    android:layout_marginRight="@dimen/dim_20"
    android:id="@+id/view1"
    ></View>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/ll_expiryDate"
            android:layout_below="@+id/tv_Plan"
            android:layout_marginLeft="@dimen/dim_20"
            android:layout_marginRight="@dimen/dim_20"
            android:layout_marginTop="@dimen/dim_10"
            >
            <TextView
                android:id="@+id/tv_expiryDateLabel"
                android:layout_width="0dp"
                android:layout_weight="50"
                android:layout_height="wrap_content"
                style="@style/textview_14_grey"
                android:textColor="@color/grey_color"
                android:text="Expiry Date"
                android:singleLine="false"
                android:maxLines="2"
                />
            <TextView
                android:id="@+id/tv_expiryDateValue"
                android:layout_width="0dp"
                android:layout_weight="50"
                android:singleLine="false"
                android:maxLines="2"
                android:layout_height="wrap_content"
                style="@style/textview_14_grey"
                android:textColor="@color/grey_color"
                android:text="Start Date"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dim_1"
            android:background="@color/notificationborder"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="@dimen/dim_20"
            android:layout_marginRight="@dimen/dim_20"
            android:id="@+id/view2"
            ></View>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/ll_cancelDate"
            android:layout_marginTop="@dimen/dim_10"
            android:layout_marginLeft="@dimen/dim_20"
            android:layout_marginRight="@dimen/dim_20"
            >
            <TextView
                android:id="@+id/tv_cancelDateLabel"
                android:layout_width="0dp"
                android:layout_weight="50"
                android:layout_height="wrap_content"
                style="@style/textview_14_grey"
                android:textColor="@color/grey_color"
                android:text="Start Date"
                android:singleLine="false"
                android:maxLines="2"
                />
            <TextView
                android:id="@+id/tv_cancelDateValue"
                android:layout_width="0dp"
                android:layout_weight="50"
                android:singleLine="false"
                android:maxLines="2"
                android:layout_height="wrap_content"
                style="@style/textview_14_grey"
                android:textColor="@color/grey_color"
                android:text="Start Date"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dim_1"
            android:background="@color/notificationborder"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="@dimen/dim_20"
            android:layout_marginRight="@dimen/dim_20"
            android:id="@+id/view3"
            ></View>
</LinearLayout>

        <TextView
            android:id="@+id/tv_Plan"
            style="@style/textview_16_grey"
            android:layout_marginTop="@dimen/dim_20"
            android:text="@string/str_no_record_found"
            android:textColor="@color/grey_color"
            android:textSize="@dimen/font_16"
            android:layout_below="@+id/iv_logo"
            android:layout_marginLeft="@dimen/dim_20"
            android:layout_marginRight="@dimen/dim_20"
            android:singleLine="false"

            android:maxLines="3"
            />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/str_subscribe"
        android:id="@+id/buyButton"
        android:layout_marginTop="@dimen/dim_30"
        android:layout_below="@+id/ll_startDate"
        android:layout_centerHorizontal="true"
        style="@style/primaryButton"
        android:background="@drawable/primary_btn_rounded_background"
        android:layout_marginRight="@dimen/dim_20"
        android:layout_marginLeft="@dimen/dim_20"
        />
    </LinearLayout>
    </androidx.core.widget.NestedScrollView>
