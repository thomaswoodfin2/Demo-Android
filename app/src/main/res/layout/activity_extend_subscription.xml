        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="@string/buyfailed_string"-->
        <!--android:id="@+id/buyfailed_string"-->
        <!--android:layout_centerVertical="true"-->
